RewriteBase /
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php


RewriteEngine On
RewriteCond %{HTTP_HOST} ecuacompus\.com [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://ecuacompus.com/$1 [R,L]
